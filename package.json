{
  "name": "react-speed-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_BAQEND=makefast-dev node scripts/start.js",
    "build": "REACT_APP_BAQEND=makefast-dev node scripts/build.js",
    "start:plesk": "REACT_APP_BAQEND=makefast node scripts/start-embedded.js",
    "build:plesk": "REACT_APP_BAQEND=makefast node scripts/build-embedded.js",
    "test": "node scripts/test.js --env=jsdom",
    "flow": "flow"
  },
  "dependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-jest": "22.0.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "baqend": "^2.9.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "^0.60.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash.merge": "^4.6.0",
    "object-assign": "4.1.1",
    "object-hash": "^1.2.0",
    "postcss-calc": "^6.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-namespace": "^1.1.1",
    "postcss-nested": "^2.1.2",
    "postcss-nested-import": "^0.1.0",
    "postcss-scopify": "^0.1.9",
    "promise": "8.0.1",
    "promise-polyfill": "^7.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^4.0.0",
    "react-css-collapse": "^3.1.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.12",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.16.0",
    "react-tippy": "^1.2.2",
    "react-toastify": "^3.3.2",
    "react-toggle": "^4.0.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-baqend": "^1.0.6",
    "redux-logger": "^3.0.6",
    "slick-carousel": "^1.8.1",
    "style-loader": "0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "tachyons": "^4.9.0",
    "typeface-open-sans": "^0.0.54",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "baqend-coding-standard": "^1.1.2",
    "eslint": "4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "^7.4.0"
  }
}
